package com.testing.org;

public class PrePostIncDec {

	
	private void preInc(int num) {
		System.out.println("Given num"+ num);
		System.out.println("Given num before pre increment"+ num);
        ++num;
        System.out.println("Given num after pre increment"+ num);
        
	}
	
	private void preDec(int num) {
		System.out.println("Given num"+ num);
		System.out.println("Given num before pre decrement"+ num);
        --num;
        System.out.println("Given num after pre decrement"+ num);
        
	}
	
	private void postInc(int num) {
		System.out.println("Given num"+ num);
		System.out.println("Given num before post increment"+ num);
        ++num;
        System.out.println("Given num after post increment"+ num);
        
	}
	
	private void postDec(int num) {
		System.out.println("Given num"+ num);
		System.out.println("Given num before post decrement"+ num);
        --num;
        System.out.println("Given num after post decrement"+ num);
        
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
