package com.testing.org;

public class Factorial {

	public static double getFactorial(int num) {
		if(num>0) {
			
			double factorial=1;
			
			for (int i = 1; i <= num; i++) {
				
				factorial*=i;
			}
			
			return factorial;
			
		}
		
		
		
		return -1;
		
	}
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println(getFactorial(20));
		
	}

}
