package com.testing.org;

 class Phone1{
	
	public void call() {
		// TODO Auto-generated method stub
System.out.println("calling from phone");
	}
	
}
class Iphone1 extends Phone1{
	
	  public void call() { // TODO Auto-generated method stub
	  System.out.println("calling from Iphone"); }
	 
	
}
class Redmi1 extends Phone1{
	public void call() {
		// TODO Auto-generated method stub
System.out.println("calling from Redmi phone");
	}
	
}


public class Downcasting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Phone1 p=new Phone1();
		p.call();
		Iphone1 ip=new Iphone1();
		ip.call();
		Redmi1 rp=new Redmi1();
		rp.call();
		
		System.out.println("------------------");
		System.out.println("Downcasting");
		System.out.println("------------------");
		/*
		 * Phone p1 =(Phone) ip; p1.call(); Phone p2=new Redmi(); p2.call();
		 */
		System.out.println(ip instanceof Iphone1);
		Iphone1 ip1= (Iphone1) p;
	
		ip1.call();
		
	}

}
